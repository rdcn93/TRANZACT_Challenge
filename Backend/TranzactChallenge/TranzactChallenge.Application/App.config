                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/TranzactChallenge.Application.Services.Period/PeriodService/" />
                    </baseAddresses>
                </host>
            </service>
            <service name="TranzactChallenge.Application.Services.Plan.PlanService">
                <endpoint address="" binding="basicHttpBinding" contract="TranzactChallenge.Application.Services.Plan.IPlanService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/TranzactChallenge.Application.Services.Plan/PlanService/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>
